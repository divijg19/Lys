{
  "name": "lys",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf .next out .velite",
    "clean:all": "rimraf .next out .velite node_modules bun.lock && bun install",
    "velite:watch": "velite dev",
    "velite:build": "velite build",
    "dev": "bun run velite:build && concurrently -k -n velite,next -c cyan,magenta \"bun run velite:watch\" \"next dev\"",
    "dev:seq": "bun run velite:build && next dev",
    "dev:turbo": "bun run velite:build && concurrently -k -n velite,next -c cyan,magenta \"bun run velite:watch\" \"cross-env NEXT_DISABLE_REACT_COMPILER=1 next dev --turbo\"",
    "dev:poll": "bun run velite:build && concurrently -k -n velite,next -c cyan,magenta \"bun run velite:watch\" \"cross-env NEXT_FORCE_POLL=1 next dev\"",
    "prebuild": "bun run type-check && bun run lint",
    "build": "bun run clean && bun run velite:build && next build",
    "build:turbo": "bun run velite:build && next build --turbopack",
    "build:analyze": "cross-env ANALYZE=true bun run build",
    "start": "next start",
    "start:prod": "bun run build && bun run start",
    "lint": "biome lint . --write",
    "lint:unsafe": "biome lint . --write --unsafe",
    "format": "biome format . --write",
    "check": "biome check .",
    "type-check": "bun run velite:build && tsc --noEmit --incremental",
    "qa": "bun run type-check && bun run lint && vitest run --reporter=dot",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run --coverage --reporter=dot",
    "test:stories": "vitest run --project storybook",
    "prepare": "husky"
  },
